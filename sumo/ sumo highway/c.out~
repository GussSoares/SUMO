# set number of nodes
set opt(nn) 30.0

# set activity file
set opt(af) $opt(config-path)
append opt(af) /a.out

# set mobility file
set opt(mf) $opt(config-path)
append opt(mf) /s.tcl

# set start/stop time
set opt(start) 0.0
set opt(stop) 500.0

# set floor size
set opt(x) 7011
set opt(y) 3016
set opt(min-x) 11
set opt(min-y) 3006
